{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__/lib",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:all": "npm run test:ci && npm run test:e2e:ci",
    "e2e:mobile:ios": "detox test --configuration ios.sim.debug",
    "e2e:mobile:ios:release": "detox test --configuration ios.sim.release",
    "e2e:mobile:android": "detox test --configuration android.emu.debug",
    "e2e:mobile:android:release": "detox test --configuration android.emu.release",
    "e2e:mobile:build:ios": "detox build --configuration ios.sim.release",
    "e2e:mobile:build:android": "detox build --configuration android.emu.release",
    "e2e:mobile:test": "detox test",
    "e2e:mobile:smoke": "detox test --testNamePattern='smoke'",
    "e2e:mobile:regression": "detox test --testNamePattern='regression'",
    "e2e:mobile:accessibility": "detox test --testNamePattern='accessibility'",
    "e2e:mobile:performance": "detox test --testNamePattern='performance'",
    "e2e:mobile:visual": "detox test --testNamePattern='visual'",
    "e2e:mobile:clean": "detox clean-framework-cache && detox build",
    "e2e:mobile:artifacts:clean": "rm -rf e2e/artifacts/*",
    "e2e:mobile:reports": "node scripts/generate-e2e-reports.js",
    "mobile:test:full": "npm run e2e:mobile:build:ios && npm run e2e:mobile:ios && npm run e2e:mobile:build:android && npm run e2e:mobile:android",
    "mobile:test:ci": "npm run e2e:mobile:test -- --headless --record-videos all --record-logs all",
    "mobile:visual:update": "VISUAL_REGRESSION=true UPDATE_BASELINES=true npm run e2e:mobile:test",
    "mobile:accessibility:test": "npm run e2e:mobile:accessibility && node scripts/analyze-accessibility.js",
    "mobile:performance:test": "npm run e2e:mobile:performance && node scripts/analyze-performance.js",
    "api:test": "node tests/api/api-test-suite.js",
    "api:test:newman": "newman run docs/api/postman-collection.json -e tests/api/environments/test.json --reporters cli,json,html",
    "api:test:comprehensive": "node scripts/test-api-comprehensive.js",
    "api:docs:generate": "node scripts/generate-api-docs.js",
    "api:docs:serve": "swagger-ui-serve docs/api/openapi.yaml",
    "api:validate": "swagger-cli validate docs/api/openapi.yaml",
    "api:performance": "artillery run tests/api/artillery-config.yml",
    "api:security": "npm audit && node scripts/security-scan.js",
    "security:test": "jest tests/security --verbose",
    "security:test:watch": "jest tests/security --watch",
    "security:test:coverage": "jest tests/security --coverage",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "node scripts/security-scan.js",
    "security:compliance": "node scripts/compliance-check.js",
    "performance:test": "jest tests/performance --verbose",
    "performance:test:watch": "jest tests/performance --watch",
    "performance:test:coverage": "jest tests/performance --coverage",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:analyze": "node scripts/performance-analyzer.js",
    "performance:benchmark": "node scripts/performance-benchmark.js",
    "performance:optimize": "node scripts/performance-optimizer.js",
    "start": "react-scripts start",
    "start:test": "cross-env NODE_ENV=test react-scripts start",
    "start:prod": "serve -s build -l 3000",
    "build": "react-scripts build",
    "test": "jest --watchAll=false",
    "test:unit": "jest tests/unit --verbose --watchAll=false",
    "test:integration": "jest tests/integration --verbose --watchAll=false",
    "test:e2e": "playwright test",
    "test:components": "jest src/components --verbose --watchAll=false",
    "test:security": "jest tests/security --verbose --watchAll=false",
    "test:performance": "jest tests/performance --verbose --watchAll=false",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "qa:analyze": "node scripts/quality-analysis.js",
    "qa:report": "node scripts/quality-report.js",
    "qa:full": "npm run qa:analyze && npm run qa:report",
    "qa:gates": "node scripts/quality-gates.js",
    "k8s:deploy": "bash scripts/k8s-deploy.sh",
    "k8s:rollback": "bash scripts/k8s-rollback.sh",
    "k8s:status": "bash scripts/k8s-status.sh",
    "docker:build": "docker build -t bloodlink/api:latest .",
    "docker:push": "docker push bloodlink/api:latest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "postinstall": "node -e \"if (require('fs').existsSync('.git')) { require('child_process').execSync('npx husky install', {stdio: 'inherit'}) } else { console.log('Skipping husky install in deployment environment') }\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
