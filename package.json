    "input-otp": "^1.4.2",
    "ioredis": "^5.4.1",
    "leaflet": "latest",
    "localforage": "latest",
    "lucide-react": "^0.534.0",
    "next": "^15.4.5",
    "next-international": "latest",
    "next-intl": "^4.3.4",
    "next-themes": "latest",
    "react": "^19",
    "react-day-picker": "^9.8.1",
